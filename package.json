{
  "name": "unexpectedly",
  "version": "0.0.7",
  "description": "Keep a bunch of test cases in a file, with their expected results",
  "main": "index.js",
  "scripts": {
    "build": "peggy -o testFile.peg.js testFile.pegjs",
    "pretest": "npm run build",
    "test": "c8 bin/unexpectedly test test/pkg_es6/test",
    "lint": "eslint --ext js,cjs,mjs . bin/*",
    "ci": "npm run lint && npm run test",
    "clean": "rimraf coverage testFile.peg.js"
  },
  "bin": {
    "unexpectedly": "bin/unexpectedly"
  },
  "keywords": [
    "test",
    "case",
    "testcase",
    "actual",
    "expected",
    "mocha",
    "harness"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "repository": {
    "type": "git",
    "url": "http://github.com/hildjj/unexpectedly.git"
  },
  "license": "MPL-2.0",
  "dependencies": {
    "mocha": "^10.2.0"
  },
  "devDependencies": {
    "@cto.af/eslint-config": "3.0.2",
    "c8": "8.0.1",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "46.9.0",
    "eslint-plugin-n": "16.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.1.0",
    "peggy": "^3.0.2",
    "rimraf": "5.0.5"
  },
  "engines": {
    "node": ">=18"
  }
}
